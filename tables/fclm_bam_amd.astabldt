	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	edbsum: fclm_bam_import_type_endof_day; // abap.char(16): Import Method For Eod Statements
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	idbsum: fclm_bam_import_type_intra_day; // abap.char(16): Import Method For Intraday Statements
	bs_upload_time: fclm_bam_import_time; // abap.tims(6): Banks Statement Check Time
	created_at: fclm_bam_obsoletedtimestamp; // abap.dec(15, 0): Timestamp
	created_by: fclm_bam_created_by; // abap.char(12): Created By

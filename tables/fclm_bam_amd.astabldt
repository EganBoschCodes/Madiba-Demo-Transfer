	@Semantics.amount.currencyCode: 'fclm_bam_amd.waers'
	cash_pool_transfer_amt_min: fclm_bam_trans_amt_min; // abap.curr(23, 2): Minimum Transfer Amount
	
	@AbapCatalog.foreignKey.keyType: #KEY
	@AbapCatalog.foreignKey.screenCheck: true
	payment_method_l: fclm_bam_payment_method_l; // abap.char(1): Payment Method For Header Account
	
	@AbapCatalog.foreignKey.keyType: #KEY
	@AbapCatalog.foreignKey.screenCheck: true
	payment_method_p: fclm_bam_payment_method_p; // abap.char(1): Payment Method For Subaccount

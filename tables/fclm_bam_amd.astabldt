	ext_rule_id: fclm_brm_rule_ext_id; // abap.char(5): Rule For Intraday Statements
	planning_level: fdlev; // abap.char(2): Planning Level
	bsr_group: fclm_bal_bsr_group; // abap.char(5): Bank Statement Group
	reconcile_endofday: fclm_bam_recon_eod; // abap.char(1): Reconcile End-Of-Day Memo Records
	sgtxt: sgtxt; // abap.char(50): Text
	
	@Semantics.amount.currencyCode: 'fclm_bam_amd.waers'
	cash_pool_transfer_amt_max: fclm_bam_trans_amt_max; // abap.curr(23, 2): Maximum Transfer Amount
	contract_type: fclm_bam_contract_type; // abap.char(2): Contract Type
	open_request_id: fclm_bam_req_id; // abap.numc(12): Request Id
	dummy_fclm_bam_amd_incl_eew_ps: cfd_dummy; // abap.char(1): Dummy

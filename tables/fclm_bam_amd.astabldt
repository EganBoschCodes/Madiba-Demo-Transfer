	bank_contact: fclm_bam_contact_person; // abap.char(10): General Contact
	rel_manager: fclm_bam_rel_manager; // abap.char(10): Relationship Manager
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	company_contact: fclm_bam_company_contact; // abap.char(12): Contact Person
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true

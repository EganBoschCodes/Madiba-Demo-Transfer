	changed_at: fclm_bam_obsoletedtimestamp; // abap.dec(15, 0): Timestamp
	changed_by: fclm_bam_changed_by; // abap.char(12): Changed By
	reviewed_at: fclm_bam_obsoletedtimestamp; // abap.dec(15, 0): Timestamp
	reviewed_by: fclm_bam_reviewed_by; // abap.char(12): Reviewed By
	closed_at: fclm_bam_obsoletedtimestamp; // abap.dec(15, 0): Timestamp
	closed_by: fclm_bam_closed_by; // abap.char(12): Closed By

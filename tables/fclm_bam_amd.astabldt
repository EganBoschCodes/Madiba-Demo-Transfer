	opened_at_tim: fclm_bam_opened_at; // abap.tims(6): Opened At
	changed_at_dat: fclm_bam_changed_on; // abap.dats(8): Changed On
	changed_at_tim: fclm_bam_changed_at; // abap.tims(6): Changed At
	closed_at_dat: fclm_bam_closed_on; // abap.dats(8): Closed On
	closed_at_tim: fclm_bam_closed_at; // abap.tims(6): Closed At

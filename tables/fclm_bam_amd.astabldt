	@AbapCatalog.foreignKey.screenCheck: true
	key mandt: mandt; // abap.clnt(3): Client
	key acc_id: fclm_bam_acc_id; // abap.numc(10): Technical Id
	key revision: fclm_revision; // abap.numc(4): Revision Number
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	banks: fclm_bam_bank_country_key; // abap.char(3): Bank Country/Region Key
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	bankl: fclm_bam_bank_key; // abap.char(15): Bank Key
	acc_num: fclm_bam_acc_num; // abap.char(40): Account Number
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
	acc_type_id: fclm_bam_bk_acc_type_id; // abap.char(10): Account Type
	valid_from: fclm_bam_amd_valid_from; // abap.dats(8): Opening Date
	valid_to: fclm_bam_amd_valid_to; // abap.dats(8): Closing Date

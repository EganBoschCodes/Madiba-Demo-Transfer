	title: ad_title; // abap.char(4): Title
	name1: ad_name1; // abap.char(40): Name
	name2: ad_name2; // abap.char(40): Name 2
	name3: ad_name3; // abap.char(40): Name 3
	name4: ad_name4; // abap.char(40): Name 4

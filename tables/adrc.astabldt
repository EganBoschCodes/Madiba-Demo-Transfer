	name_co: ad_name_co; // abap.char(40): C/O
	city1: ad_city1; // abap.char(40): City
	city2: ad_city2; // abap.char(40): District
	
	@AbapCatalog.foreignKey.keyType: #NON_KEY
	@AbapCatalog.foreignKey.screenCheck: true
